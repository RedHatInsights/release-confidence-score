# GitHub configuration
GITHUB_TOKEN=your_github_token_here

# GitLab configuration
#GITLAB_BASE_URL=https://gitlab.cee.redhat.com/
#GITLAB_SKIP_SSL_VERIFY=true
GITLAB_TOKEN=your_gitlab_token_here

# Logging configuration
#LOG_FORMAT=json
#LOG_LEVEL=info

# Model selection (claude, gemini or llama)
#MODEL_PROVIDER=claude

# Claude configuration - REQUIRED when using MODEL_PROVIDER=claude
CLAUDE_MODEL_API=https://your-claude-api-host.example.com
CLAUDE_MODEL_ID=claude-sonnet-4@20250514
CLAUDE_USER_KEY=your_claude_api_key_here

# Gemini configuration - REQUIRED when using MODEL_PROVIDER=gemini
#GEMINI_MODEL_API=https://your-gemini-api-host.example.com
#GEMINI_MODEL_ID=gemini-2.5-pro
#GEMINI_USER_KEY=your_gemini_api_key_here

# Llama configuration - REQUIRED when using MODEL_PROVIDER=llama
#LLAMA_MODEL_API=https://your-llama-api-host.example.com
#LLAMA_MODEL_ID=RedHatAI/Llama-3.3-70B-Instruct-FP8-dynamic
#LLAMA_USER_KEY=your_llama_api_key_here

# Additional model configuration
#MODEL_MAX_RESPONSE_TOKENS=2000
#MODEL_SKIP_SSL_VERIFY=true
#MODEL_TIMEOUT_SECONDS=120

# System prompt version
#SYSTEM_PROMPT_VERSION=v1

# Scoring thresholds
#SCORE_THRESHOLD_AUTO_DEPLOY=80
#SCORE_THRESHOLD_REVIEW_REQUIRED=60
